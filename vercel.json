{
  "version": 2,
  "builds": [
    {
      "src": "api/simular.py",
      "use": "@vercel/python"
    },
    {
      "src": "app/**/*.tsx",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/simular.py"
    },
    {
      "src": "/(.*)",
      "dest": "/app/$1"
    }
  ]
}
